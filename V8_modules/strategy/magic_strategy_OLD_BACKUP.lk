                        var dgUses = min(dg._maxUse, floor(player._currTp / dg._cost))
                        while (dgUses > 0 && player._currTp >= dg._cost) {
                            push(this._actions, new Action(Action.ACTION_DIRECT, dg._id, -1, target._cellPos, target))
                            player._currTp -= dg._cost
                            dgUses -= 1
                            willAttack = true
                        }
                    }
                }

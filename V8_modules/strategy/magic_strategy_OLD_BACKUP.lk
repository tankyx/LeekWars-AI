                if (dg != null && this.inRangeAndLOS(targetHitCell._id, target._cellPos, dg._minRange, dg._maxRange)) {
                    var needSwapDG = (getWeapon() != dg._id)
                    if (player._currTp >= dg._cost + (needSwapDG ? 1 : 0)) {
